{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "headers": [
    {
      "source": "/manifest.webmanifest",
      "headers": [
        {
          "key": "content-type",
          "value": "application/manifest+json"
        }
      ]
    },
    {
      "source": "/LICENSE",
      "headers": [
        {
          "key": "content-type",
          "value": "text/plain"
        }
      ]
    }
  ]
}
