{
  "exclude": ["node_modules"],
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "types": ["@cloudflare/workers-types"],
    "jsx": "preserve",
    "target": "ES2018",
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
